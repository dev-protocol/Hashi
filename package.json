{
  "name": "hashi",
  "version": "0.1.1",
  "description": "Dev Protocol's design system implementation for the web.",
  "main": "index.js",
  "repository": "https://github.com/dev-protocol/hashi-web",
  "author": "Dev Protocol",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:ts": "webpack --config webpack.config.js",
    "build:ts:watch": "webpack --config webpack.config.js --watch",
    "build:sass": "gulp",
    "publish:public": "cd src && npm publish --access=public",
    "bump": "npm --no-git-tag-version version patch && cd src && npm --no-git-tag-version version patch"
  },
  "dependencies": {
    "@matteusan/sentro": "0.1.20"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-cli": "2.3.0",
    "gulp-purgecss": "4.1.3",
    "gulp-sass": "5.1.0",
    "sass": "1.49.0",
    "ts-loader": "9.2.6",
    "typescript": "4.5.5",
    "webpack": "5.67.0",
    "webpack-cli": "4.9.2"
  }
}
